        { "keys": ["Prefix &"], "desc": "Kill current window", "tags": ["window", "danger"] },
        { "keys": ["Prefix n"], "desc": "Next window", "tags": ["window"] },
        { "keys": ["Prefix p"], "desc": "Previous window", "tags": ["window"] },
        { "keys": ["Prefix l"], "desc": "Last active window", "tags": ["window"] },
        { "keys": ["Prefix w"], "desc": "Choose window", "tags": ["window"] },
        { "keys": ["Prefix 0â€“9"], "desc": "Select window by index", "tags": ["window"] },
        { "keys": ["Prefix ."], "desc": "Move window (prompt for index)", "tags": ["window"] }
      ]
    },
    {
      "title": "Panes",
      "items": [
        { "keys": ["Prefix %"], "desc": "Split pane vertically", "tags": ["pane", "split"] },
