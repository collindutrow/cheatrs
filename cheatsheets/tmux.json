{
  "id": "tmux",
  "name": "tmux",
  "description": "Session, window, pane management and copy mode.",
  "hint": "Prefix key defaults to Ctrl+b. Commands are prefix-based unless stated otherwise.",
  "processes": ["tmux", "tmux.exe", "WindowsTerminal.exe", "cmd.exe", "consolehost.exe"],
  "sections": [
    {
      "title": "Session / Client",
      "items": [
        { "keys": ["tmux"], "desc": "Start tmux server and client", "tags": ["session"] },
        { "keys": ["tmux new -s name"], "desc": "Create new session with name", "tags": ["session"] },
        { "keys": ["tmux ls"], "desc": "List sessions", "tags": ["session"] },
        { "keys": ["tmux attach -t name"], "desc": "Attach to session", "tags": ["session"] },
        { "keys": ["Prefix d"], "desc": "Detach from session", "tags": ["session"] },
        { "keys": ["Prefix s"], "desc": "Choose session", "tags": ["session"] },
        { "keys": ["Prefix $"], "desc": "Rename current session", "tags": ["session"] },
        { "keys": ["Prefix :kill-session"], "desc": "Kill current session", "tags": ["session", "danger"] }
      ]
    },
    {
      "title": "Windows",
      "items": [
        { "keys": ["Prefix c"], "desc": "Create new window", "tags": ["window"] },
