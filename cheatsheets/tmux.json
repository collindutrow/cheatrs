        { "keys": ["Prefix :show-buffer"], "desc": "Show buffer contents", "tags": ["buffer"] },
        { "keys": ["Prefix :list-buffers"], "desc": "List paste buffers", "tags": ["buffer"] },
        { "keys": ["Prefix :delete-buffer"], "desc": "Delete buffer", "tags": ["buffer", "danger"] }
      ]
    },
    {
      "title": "Command / Misc",
      "items": [
        { "keys": ["Prefix :"], "desc": "Command prompt", "tags": ["command"] },
        { "keys": ["Prefix t"], "desc": "Show clock", "tags": ["misc"] },
        { "keys": ["Prefix ?"], "desc": "List key bindings", "tags": ["help"] },
        { "keys": ["Prefix r"], "desc": "Reload tmux configuration", "tags": ["config"] },
        { "keys": ["Prefix Ctrl+b"], "desc": "Send prefix to nested tmux", "tags": ["misc"] }
      ]
    }
  ]
}
