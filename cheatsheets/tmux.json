        { "keys": ["Prefix Ctrl+h/j/k/l"], "desc": "Resize pane in direction", "tags": ["pane", "resize"] },
        { "keys": ["Prefix Alt+1"], "desc": "Even-horizontal layout", "tags": ["pane", "layout"] },
        { "keys": ["Prefix Alt+2"], "desc": "Even-vertical layout", "tags": ["pane", "layout"] },
        { "keys": ["Prefix Alt+3"], "desc": "Main-horizontal layout", "tags": ["pane", "layout"] },
        { "keys": ["Prefix Alt+4"], "desc": "Main-vertical layout", "tags": ["pane", "layout"] },
        { "keys": ["Prefix ="], "desc": "Equalize pane sizes", "tags": ["pane", "layout"] }
      ]
    },
    {
      "title": "Copy mode / Scrollback",
