        { "keys": ["Prefix x"], "desc": "Kill current pane", "tags": ["pane", "danger"] },
        { "keys": ["Prefix o"], "desc": "Select next pane", "tags": ["pane"] },
        { "keys": ["Prefix ;"], "desc": "Toggle last active pane", "tags": ["pane"] },
