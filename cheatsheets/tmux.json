        { "keys": ["q"], "desc": "Exit copy mode", "tags": ["copy"] },
        { "keys": ["Up/Down"], "desc": "Scroll line up/down", "tags": ["copy", "scroll"] },
        { "keys": ["PageUp/PageDown"], "desc": "Scroll page up/down", "tags": ["copy", "scroll"] },
